<!DOCTYPE html>
<html lang="pt">
<head>
    <meta charset="UTF-8">
</head>
<body>

<script>
    let paises = [
        'Albânia',
        'Bolívia',
        'Canadá',
        'Dinamarca',
        'Etiópia',
        'Finlândia',
        'Alemanha',
        'Hungria',
        'Irlanda',
        'Japão',
        'Quênia'
    ];

    for (let i = 0; i < paises.length; i++) {
        console.log(paises[i]);
    }

    function contarPaises() {
        return paises.length;
    }

    console.log(contarPaises());

    paises.unshift("Portugal");
    paises.push("Espanha");

    function ultimosTres() {
        console.log(paises.slice(-3));
    }

    ultimosTres();

    console.log(paises.join(" - "));
</script>

</body>
</html>

